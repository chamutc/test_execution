{
  "name": "jenkins-test-scheduler",
  "version": "1.0.0",
  "description": "A comprehensive web-based Jenkins test scheduler application that manages test sessions, hardware resources, and machine allocation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "dev-all": "concurrently \"npm run dev\" \"npm run frontend\"",
    "test": "jest"
  },
  "keywords": [
    "jenkins",
    "test-scheduler",
    "hardware-management",
    "scheduling",
    "csv-import"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}
